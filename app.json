{
  "expo": {
    "name": "PucciApp",
    "description": "PucciApp!",
    "owner": "pucciapp",
    "slug": "pucciapp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "android"
    ],
    "android": {
      "versionCode": 2,
      "package": "com.bb.pucciapp"
    },
